# Start up docker server on localhost
$ docker run -p 7474:7474 -p 7687:7687 -v C:/Users/Admin/neo4j/data:/data -v C:/Users/Admin/neo4j/plugins:/plugins -e NEO4J_apoc_export_file_enabled=true -e NEO4J_apoc_import_file_enabled=true -e NEO4J_apoc_import_file_use__neo4j__config=true -e NEO4JLABS_PLUGINS=\[\"apoc\"\] --env=NEO4J_AUTH=none neo4j

# Start webclient
$ python webclient.py
